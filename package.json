{
  "name": "geoblaze",
  "version": "0.2.6",
  "description": "Blazing Fast JavaScript Raster Processing Engine",
  "main": "dist/geoblaze.node.min.js",
  "browser": {
    "./dist/geoblaze.web.min.js": "./dist/geoblaze.web.min.js"
  },
  "unpkg": "./dist/geoblaze.web.min.js",
  "scripts": {
    "analyze": "webpack --config webpack.analyze.js",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "npm run build:dev:node && npm run build:dev:web",
    "build:dev:node": "webpack --mode development --target node",
    "build:dev:web": "webpack --mode development --target web",
    "build:prod": "npm run build:prod:node && npm run build:prod:web",
    "build:prod:node": "webpack --mode production --target node",
    "build:prod:web": "webpack --mode production --target web",
    "dev": "webpack --mode development --target node --watch",
    "documentation": "./node_modules/.bin/documentation build src/** --config documentation.yml -f html -o docs && echo 'geoblaze.io' > docs/CNAME",
    "fix": "eslint src --fix",
    "lint": "eslint src",
    "server": "node ./serve.js",
    "test": "./node_modules/.bin/mocha src --recursive --require @babel/register",
    "test-loading-builds": "./node_modules/.bin/mocha test-loading-builds.js",
    "setup": "bash setup.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeoTIFF/geoblaze.git"
  },
  "keywords": [
    "geoblaze",
    "georaster",
    "geotiff",
    "geotiff.io",
    "gis",
    "maps",
    "raster",
    "rasters",
    "geoprocessing",
    "pixels",
    "tiff"
  ],
  "author": "Daniel Dufour and Steve Peyton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GeoTIFF/geoblaze/issues"
  },
  "homepage": "https://github.com/GeoTIFF/geoblaze#readme",
  "dependencies": {
    "@turf/combine": "^4.7.3",
    "georaster": "^1.0.3",
    "mathjs": "^6.6.1",
    "node-fetch": "^2.6.0",
    "proj4": "^2.6.0",
    "terraformer-arcgis-parser": "^1.0.5",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@turf/bbox-polygon": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "documentation": "^9.1.1",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.4",
    "jsdoc": "^3.6.3",
    "memory-fs": "^0.4.1",
    "mocha": "^7.1.1",
    "path": "^0.12.7",
    "puppeteer": "^1.11.0",
    "shapefile": "^0.6.6",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.1.3"
  }
}
